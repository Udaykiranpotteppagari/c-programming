#include<stdio.h>
int main(){
    int a[10][10],i,n,o,j;
    printf("Enter the size of rows: ");
    scanf("%d",&n);
    printf("Enter the size of columns: ");
    scanf("%d",&o);
    printf("Enter the elements into array\n");
    for(i=0;i<n;i++){
        for(j=0;j<o;j++){
            scanf("%d",&a[i][j]);
        }
    }
    printf("The 2D matrix is:\n");
     for(i=0;i<n;i++){
        for(j=0;j<o;j++){
            printf("%d ",a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
