#include<stdio.h>

int main(){

    int a[100],i,n;
    printf("Enter the size of array:");
    scanf("%d",&n);
    printf("Enter elements into array:\n");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("The entered elements are: ");
    for(i=0;i<n;i++){
        printf("%d\t",a[i]);
    }
    return 0;
}
