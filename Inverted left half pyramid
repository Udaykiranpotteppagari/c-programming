#include <stdio.h>

void invertedLeftHalfPyramid(int a) {
    for (int b = 1; b <= a; b++) {
        for (int c = 1; c < b; c++) {
            printf(" ");
        }
        for (int c = b; c <= a; c++) {
            printf("*");
        }
        printf("\n");
    }
}

int main() {
    int a = 5;
    invertedLeftHalfPyramid(a);
    return 0;
}
